{
  "productName": "TinyPNG-rs",
  "version": "0.1.0-alpha",
  "identifier": "com.rusttinypng.app",
  "build": {
    "beforeDevCommand": "./dev-frontend.sh",
    "frontendDist": "../dist"
  },
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "TinyPNG-rs",
        "width": 800,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 600,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
