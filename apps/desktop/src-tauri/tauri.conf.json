{
  "productName": "Panda Pixel",
  "version": "0.1.0-alpha",
  "identifier": "com.pandapixel.app",
  "build": {
    "beforeDevCommand": "cd ../../apps/web && npm run build:css",
    "frontendDist": "../../../dist"
  },
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Panda Pixel",
        "width": 800,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 600,
        "minHeight": 700
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": false
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}